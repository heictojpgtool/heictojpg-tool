{
  "version": 2,
  "builds": [
    { "src": "api/convert.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/convert", "dest": "api/convert.js" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/style.css", "dest": "/style.css" }
  ]
}


